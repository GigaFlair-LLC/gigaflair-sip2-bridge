{
  "name": "sip2-json",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch --ignore data src/server.ts",
    "build": "tsc",
    "start": "node dist/bin/start.js",
    "mock:lms": "tsx watch src/mock-lms.ts",
    "test": "vitest run",
