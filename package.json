    "@fastify/rate-limit": "^10.3.0",
    "@fastify/session": "^11.1.1",
    "@fastify/static": "^9.0.0",
    "argon2": "^0.44.0",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "unidecode": "^1.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^25.3.1",
    "@types/selfsigned": "^2.0.4",
    "@types/unidecode": "^1.1.0",
    "eslint": "^10.0.1",
    "selfsigned": "^5.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
